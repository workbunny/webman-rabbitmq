{
  "name": "workbunny/webman-rabbitmq",
  "type": "library",
  "license": "MIT",
  "description": "Webman plugin workbunny/webman-rabbitmq",
  "authors": [
    {
      "name": "chaz6chez",
      "email": "chaz6chez1993@outlook.com",
      "homepage": "https://chaz6chez.cn"
    }
  ],
  "support": {
    "issues": "https://github.com/workbunny/webman-rabbitmq/issues",
    "source": "https://github.com/workbunny/webman-rabbitmq"
  },
  "require": {
    "php": "^8.1",
    "bunny/bunny": "^0.5",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "workerman/webman-framework": "^2.0",
    "workerman/workerman": "^2.0",
    "webman/console": "^2.0",
    "phpunit/phpunit": "^10.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "symfony/var-dumper": "^6.0 | ^7.0",
    "revolt/event-loop": "^1.0",
    "swow/swow-stub": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Workbunny\\WebmanRabbitMQ\\": "src",
      "Protocols\\" : "src/Connection/Protocol"
    },
    "files": [
      "./src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Workbunny\\Tests\\": "tests",
      "process\\": "process"
    }
  },
  "suggest": {
    "revolt/event-loop": "supported Event/Fiber",
    "ext-swoole": "supported Event/Swoole",
    "ext-swow": "supported Event/Swow",
    "webman/console": "supported builder commands"
  },
  "scripts": {
    "tester": "php tests/tester start",
    "fmt": "./vendor/bin/php-cs-fixer fix",
    "doc-install": "@php -r \"shell_exec('curl -sSLf -o /usr/local/bin/phpDocumentor https://phpdoc.org/phpDocumentor.phar && chmod +x /usr/local/bin/phpDocumentor');\"",
    "doc": "phpDocumentor"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev"
    }
  }
}
