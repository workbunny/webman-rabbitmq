<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/phpDocumentor/phpDocumentor/master/data/xsd/phpdoc.xsd"
>
    <setting name="template.color" value="blue"/>
    <paths>
        <output>docs</output>
        <cache>.phpdoc</cache>
    </paths>
    <versions>
        <version number="latest">
            <api>
                <source dsn="src">
                    <path>Builders</path>
                    <path>channels</path>
                    <path>Clients</path>
                    <path>Commands</path>
                    <path>Connections</path>
                    <path>Exceptions</path>
                    <path>Traits</path>
                    <path>*.php</path>
                </source>
                <ignore hidden="true" symlinks="true">
                    <path>src/config</path>
                    <path>Install.*</path>
                </ignore>
                <extensions>
                    <extension>php</extension>
                </extensions>
                <visibility>public</visibility>
                <default-package-name>WebmanRabbitMQ</default-package-name>
                <include-source>true</include-source>
                <markers>
                    <marker>TODO</marker>
                    <marker>FIXME</marker>
                </markers>
            </api>
        </version>
    </versions>
</phpdocumentor>